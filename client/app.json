{
  "expo": {
    "userInterfaceStyle": "automatic",
    "scheme": "acme",
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow protectHER to access your camera?",
          "microphonePermission": "Allow protectHER to access your microphone?"
        }
      ]
    ],
    "icon": "./assets/icon.png",
    "orientation": "portrait",
    "android": {
      "userInterfaceStyle": "automatic",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "ios": {
      "userInterfaceStyle": "automatic"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#00B4FF"
    },
    "web": {
      "bundler": "metro"
    },
    "name": "client",
    "slug": "client"
  }
}
